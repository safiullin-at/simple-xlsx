{
    "name": "safiullin-at/simple-xlsx",
    "description": "xlsx parser",
    "keywords": [
        "xlsx", "parser"
    ],
    "homepage": "https://github.com/safiullin-at/simple-xlsx",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Safiullin",
            "email": "safiullin.at@yandex.ru",
            "role": "PHP developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
